   "source": [
    "import numpy as np\n",
    "import cv2 \n",
    "pic = cv2.imread('Images/op.jpg')\n",
    "cols = pic.shape[1]\n",
    "rows = pic.shape[0]\n",
    "M = np.float32([[1,0,50],[0,1,50]])\n",
    "dst = cv2.warpAffine(pic,M,(cols,rows))\n",
    "cv2.imshow('dst',dst)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
